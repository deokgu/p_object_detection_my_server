{
    "name": "Object_detection",
    "type": "detection",
    "n_gpu": 1,
    "save": true,
    "batch_size": 8,
    "num_workers": 4,
    "score_threshold": 0.5,
    "fold_split": 5,
    "data_set": {
        "type": "CustomCOCODataset",
        "args": {
            "annotation": "/opt/ml/detection/dataset/train.json",
            "data_dir": "/opt/ml/detection/dataset"
        }
    },
    "Net": {
        "type": "custom_efficientdet",
        "args": {
            "det_name": "tf_efficientdet_d7",
            "num_classes": 10,
            "image_size": [
                512,
                512
            ]
        }
    },
    "transform": {
        "type": "CustomTransForm",
        "args": {
            "mean": [
                0.548,
                0.504,
                0.479
            ],
            "std": [
                0.237,
                0.247,
                0.246
            ],
            "resize": [
                512,
                512
            ]
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "nll_loss",
    "metrics": [],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [
                10,
                15,
                20,
                25,
                30,
                35,
                40,
                45
            ],
            "gamma": 0.05
        }
    },
    "trainer": {
        "epochs": 5,
        "save_dir": "logs/",
        "save_period": 5,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboard": false
    }
}